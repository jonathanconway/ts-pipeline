{
	"compilerOptions": {
	  "sourceMap": true,
	  "target": "es5",
	  "noUnusedLocals": true,
	  "noUnusedParameters": true,
	  "strictNullChecks": true,
	  "noFallthroughCasesInSwitch": true,
	  "strict": true,
	  "jsx": "react",
	  "declaration": true,
	  "lib": [
		"dom",
		"es2015"
	  ],
	  "baseUrl": "."
	},
	"include": [
	  "src/**/*",
	  "typings/**/*"
	]
}